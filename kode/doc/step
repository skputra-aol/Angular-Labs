$ sudo docker run -e "ACCEPT_EULA=Y" -e "MSSQL_SA_PASSWORD=P@ssw0rd" \
   -p 1433:1433 --name serversql1 --hostname serversql1 \
   -d \
   mcr.microsoft.com/mssql/server:2022-latest
$ sudo docker start -a
$ sudo docker ps -a
$ sudo docker exec -it serversql1 "bash"
$ /opt/mssql-tools/bin/sqlcmd -S localhost -U SA -P "P@ssw0rd"
$ SELECT @@SERVERNAME
$ create database dbputra
$ use dbputra
$ create table tb1(id int identity(1,1) primary key , nama nvarchar(50), note nvarchar(100))
$ go
$ exit

